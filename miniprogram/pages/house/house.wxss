/* miniprogram/pages/house/house.wxss */
.round-click{
  height: 125rpx;
  width: 125rpx;
  border-radius: 100%;
  position: fixed;
  bottom: 250rpx;
  right: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 99;
}
.round-click navigator{
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}
.line-limit-2 {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  color: rgba(21, 21, 148, 0.185);
}

.itemtitle{
  font-size: 27rpx;
}

.placeholder-buttom {
  width: 100%;
  height: 90rpx;
}

.placeholder-mid {
  width: 100%;
  height: 10rpx;
}
