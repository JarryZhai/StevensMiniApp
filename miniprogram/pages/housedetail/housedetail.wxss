/* miniprogram/pages/housedetail/housedetail.wxss */
.pageTwo .swiperClass {margin: 0;margin-top: 10px;}
.pageTwo .slide-image {width: 100%;height: 100%;border-radius: 10px;position: relative;box-shadow: 0 0 10rpx rgba(0, 0, 0, .8)}
.pageTwo image.active {transform: none;transition: all 0.6s ease-in 0s;}
.pageTwo image.quiet {transform: scale(0.7);transition: all 0.6s ease-in 0s;}


.title{
  width: 100%;
  white-space:pre-line;
  white-space:pre-wrap;
  font-weight: bold;
  margin: 2.5rpx 0.5rpx 2.5rpx 0.5rpx;
  word-break: break-all;
  text-align: left
}
